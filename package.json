{
  "name": "@google/clasp",
  "version": "2.4.2",
  "description": "Develop Apps Script Projects locally",
  "type": "module",
  "exports": "./build/src/index.js",
  "main": "build/src/index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bin": {
    "clasp": "build/src/index.js"
  },
  "files": [
    "docs",
    "build/src"
  ],
  "scripts": {
    "build": "npm run compile && npm i -g --loglevel=error --force",
    "build-fresh": "npm cache clean --force && npm i && npm run build",
    "watch": "tsc --project tsconfig.json --watch",
    "prepare": "npm run compile",
    "check": "npm run lint",
    "test": "nyc mocha --cache false --timeout 100000 --recursive build/test",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "prettier": "prettier src test --write",
    "lint": "gts lint src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "pretest": "npm run compile && npm run lint"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/clasp"
  },
  "keywords": [
    "Apps",
    "Script",
    "SDK",
    "API",
    "script.google.com",
    "extension",
    "add-on"
  ],
  "author": "Grant Timmerman",
  "license": "Apache-2.0",
  "dependencies": {
    "@sindresorhus/is": "^4.6.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-truncate": "^3.1.0",
    "commander": "^9.5.0",
    "debounce": "^1.2.1",
    "dotf": "^2.0.2",
    "find-up": "^6.3.0",
    "fs-extra": "^9.1.0",
    "fuzzy": "^0.1.3",
    "gaxios": "^5.1.3",
    "google-auth-library": "^7.14.1",
    "googleapis": "^100.0.0",
    "inquirer": "^8.2.6",
    "inquirer-autocomplete-prompt-ipt": "^2.0.0",
    "is-reachable": "^5.2.1",
    "log-symbols": "^5.1.0",
    "loud-rejection": "^2.2.0",
    "make-dir": "^4.0.0",
    "multimatch": "^6.0.0",
    "normalize-newline": "^4.1.0",
    "open": "^8.4.2",
    "ora": "^6.3.1",
    "p-map": "^5.5.0",
    "read-pkg-up": "^9.1.0",
    "recursive-readdir": "^2.2.3",
    "server-destroy": "^1.0.1",
    "split-lines": "^3.0.0",
    "strip-bom": "^5.0.0",
    "ts2gas": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/debounce": "^1.2.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.20.55",
    "@types/recursive-readdir": "^2.2.4",
    "@types/server-destroy": "^1.0.3",
    "@types/tmp": "^0.2.6",
    "chai": "^4.3.10",
    "coveralls": "^3.1.1",
    "gts": "^4.0.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "tmp": "^0.2.1",
    "type-fest": "^2.19.0"
  }
}
